<h1>店舗登録画面</h1>
<%= form_with model: @shop, local:true do |f| %>
  <h4>店舗外観画像</h4>
  <%= f.attachment_field :image %>
  <h4>ショップ名</h4>
  <%= f.text_field :name %>
  <h4>店舗概要</h4>
  <%= f.text_area :introduction %>
  <h4>電話番号</h4>
  <%= f.text_area :telephone_number %>
  <h4>住所</h4> 
  <%= f.text_area :address %>
  <h4>お店のURL</h4> 
  <%= f.text_area :url %>
  <%= f.submit '投稿' %>
<% end %>